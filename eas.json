{
  "cli": {
    "version": ">= 3.15.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "SSH_KEY": "@SSH_KEY"
      }
    },
    "production": {
      "node": "18.17.1",
      "yarn": "1.22.19",
      "distribution": "store",
      "cache": {
        "disabled": true
      },
      "env": {
        "SSH_KEY": "@SSH_KEY",
        "ENVIRONMENT": "production"
      },
      "ios": {
        "simulator": false,
        "image": "latest",
        "env": {
          "PLATFORM": "ios"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
